  bgImage="picture/fon1.png"
  iconSize="100"
  font="15pt"
  minWidth="250"
  maxWidth="300"
  >

    <item text="widget mnemo" icon="picture/picture_1.png">
      <scrollArea align="Center" bgColor="rgb(102,102,102)">
        <widget file="testmnemowidget.xml"/>
      </scrollArea>
    </item>

    <item text="widget tab" icon="picture/picture_1.png">
      <tabs file="testtab.xml"/>
    </item>

    <item text="widget 2" icon="picture/icon1.png">
