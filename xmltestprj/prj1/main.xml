    </WIDGETBUILDERS>
    
    <WIDGETBUILDERS file="widgetbuilders.xml"/>
    
    <LAYOUTBUILDERS>
        <BUILDER name="layoutGrid" lib="libs/layoutsbase" libhandler="getGridLayoutBuilder" />
        <BUILDER name="layoutVBox" lib="libs/layoutsbase" libhandler="getVBoxLayoutBuilder" />
    </LAYOUTBUILDERS>

    <LAYOUTBUILDERS file="layoutbuilders.xml"/>
    
    
    <WINDOWS file="windows.xml"/>
    
    <WINDOWS>
        <WINDOW name="dialog2" visible="false" file="dialog2.xml"/>
