<?xml version="1.0" encoding="UTF-8" ?>

<APPLICATION>

  <SOURCES>
    <Localsources>
      <source id="localsource" mapfile="localsourcemap.xml"  />
    </Localsources>
  </SOURCES>

  <KEYBOARD 
    id="keyboard" 
    window="keyboard_window"
    source="localsource" 
    stream="keyboard_stream"
    data="keyboard_data" />

  <WINDOW 
    id="keyboard_window" 
    file="keyboard_window.xml" 
    visible="false"
    title="Digital Keyboard" 
    showMode="normal"
    modality="true"
    flags="stayOnTop" 
    position="200;200" />

  <WINDOW 
    id="main_window" 
    file="main_window.xml"
    visible="true"
    title="Window 1" 
    showMode="normal"
    modality="false"
    flags="" 
    position="100;100" 
    size="500;200"
    screen="0"/>

</APPLICATION>

